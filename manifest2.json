{
    "manifest_version": 3,
    "name": "BypassRequest",
    "description": "Copy and Paste a Bypass Code Request for your SL.",
    "version": "1.0",
    "content_scripts": [
        {
            "matches": ["http://*/*", "https://*/*"],
            "js": ["content.js"]
        }
    ],
    "background": {
        "js": "background.js"
    },
    "action": {
        "default_popup": "popup.html"
        }
    }